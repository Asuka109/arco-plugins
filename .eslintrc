{
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "env": {
    "node": true
  },
  "rules": {
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "consistent-return": 0,
    "no-plusplus": 0,
    "no-continue": 0,
    "no-param-reassign": 0,
    "lines-between-class-members": 0,
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0,
    "no-useless-catch": 0,
    "prefer-destructuring": 0
  }
}
